{% extends 'base.html.twig' %}

 {% block title %}<title>HOME PAGE</title>{% endblock title %}
    {% block css %}<link rel="stylesheet" href="{{ asset("assets/css/autre.css") }}"/>{% endblock css %}
 
{% block header %}
<h1 class="mt-4">Bienvenue sur CrossfitSAB</h1>
{% endblock header %}
{% block body %}

<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img id="carrousel1" height="500" width="100" src="https://cdn.pixabay.com/photo/2017/01/09/11/30/dumbbell-1966247__480.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img id="carrousel2" height="500" src="https://cdn.pixabay.com/photo/2018/04/04/16/44/kettlebell-3290296__480.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img id="carrousel2" height="500" src="https://cdn.pixabay.com/photo/2018/07/17/21/36/fitness-3545063_1280.jpg" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>





{% endblock body %}